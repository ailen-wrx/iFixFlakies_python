$ cut -d, -f5,7,8 summary/Test_Status.csv | grep True,brittle,True | wc -l
     283    # brittle with at least one state-setter
$ cut -d, -f5,7,8 summary/Test_Status.csv | grep True,brittle,False | wc -l
      35    # brittle with state-setter undetected
$ cut -d, -f5,7,8,9 summary/Test_Status.csv | grep True,victim,True,True | wc -l
     242    # victim with polluter and corresponding cleaner
$ cut -d, -f5,7,8,9 summary/Test_Status.csv | grep True,victim,True,False | wc -l
     311    # victim with polluter but cleaner undetected
$ cut -d, -f5,7,8,9 summary/Test_Status.csv | grep True,victim,False,False | wc -l
     247    # victim with polluter undetected
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,True,victim | wc -l 
      76    # victim with patch generated
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,False,victim | wc -l
     724    # victim with no patch generated
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,True,brittle | wc -l
      92    # brittle with patch generated
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,False,brittle | wc -l
     226    # brittle with no patch generated
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,True | wc -l   
     204    # flaky test with patch generated
$ cut -d, -f5,6,7 summary/Test_Status.csv | grep True,True,NOD | wc -l
      36    # flaky test marked as NOD but still has patch
$ cut -d, -f5,7 summary/Test_Status.csv | grep False,passed | wc -l
     453    # flaky tests claimed as victim but not detected by ipflakies
$ cut -d, -f5,7 summary/Test_Status.csv | grep False,failed | wc -l
     179    # flaky tests brittle as victim but not detected by ipflakies
$ cut -d, -f5,7 summary/Test_Status.csv | grep True,victim | wc -l
     800    # Detected victim
$ cut -d, -f5,7 summary/Test_Status.csv | grep True,brittle | wc -l
     318    # Detected brittle
$ cut -d, -f5,7 summary/Test_Status.csv | grep True,NOD | wc -l
     225    # Detected NOD
$ cut -d, -f5 summary/New_Found.csv| grep NOD | wc -l            
     362    # Detected NOD but not claimed by Gruber
$ cut -d, -f5 summary/New_Found.csv| grep victim | wc -l         
